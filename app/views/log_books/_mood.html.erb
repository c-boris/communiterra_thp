<% if log_book.mood == "no_data" %>
  <%= image_tag("/assets/shrug.png", width: "32", height: "32", alt: "smiley rouge pas content") %>
<% elsif log_book.mood == "awful" %>
  <%= image_tag("/assets/angry.png", width: "32", height: "32", alt: "smiley rouge pas content") %>
<% elsif log_book.mood == "bad" %>
  <%= image_tag("/assets/mild.png", width: "32", height: "32", alt: "smiley rouge pas content") %>
<% elsif log_book.mood == "okay" %>
  <%= image_tag("/assets/neutral.png", width: "32", height: "32", alt: "smiley rouge pas content") %>
<% elsif log_book.mood == "great" %>
  <%= image_tag("/assets/smiling.png", width: "32", height: "32", alt: "smiley rouge pas content") %>
<% else %>
  <%= image_tag("/assets/happy.png", width: "32", height: "32", alt: "smiley rouge pas content") %>
<% end %>
